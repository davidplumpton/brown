{
    "name": "Brown",
    "manifest_version": 2,
    "version": "0.6",
    "description": "Browsing Nirvana: Finds the next page.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["common.js", "bg.js"]
    },
    "permissions": [
        "tabs", "notifications"
    ],
    "options_page": "popup.html",
    "commands": {
        "code_brown": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y",
                "mac": "MacCtrl+Shift+Y"
            },
            "description": "Load next page in a tab"
	}
    }
}
